@model EcommerceMVC.EcommerceDTOs.LogsResponseModel
@inject IHttpContextAccessor httpContextAccessor

@{
    ViewData["Title"] = "Logs";
}
<h1>@ViewData["Title"]</h1>

<p>Logs:</p>

<center>
<table class="table table-bordered table-responsive-md table-hover">
    <tr>
        <th class="row-cols-3">Log Id</th>
        <th class="row-cols-3">Log Description</th>
        <th class="row-cols-3">Added By</th>
        <th class="row-cols-3">Log Date</th>
        <th class="row-cols-3">Session</th>
    </tr>
    @foreach(var logs in @Model.Logs)
    {
    <tr>
        <td class="row-cols-3">@logs.LogId</td>
        <td class="row-cols-3">@logs.LogDescription</td>
        <td class="row-cols-3">@logs.AddedBy</td>
        <td class="row-cols-3">@logs.LogDate</td>
        <td class="row-cols-3">@httpContextAccessor.HttpContext.Session.GetString("SessionKeyUsername")</td>
    </tr>
    }
</table>
</center>
